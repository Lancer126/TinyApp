<h1>TinyApp</h1>

<% if (!user) { %>
    <a href="<%= '/login' %>">Login</a>
     | 
    <a href="<%= '/register' %>">Register</a><br>
    <a href="<%= '/urls/new' %>">Create New TinyUrl</a><br>
<% } else { %>
    <a href="<%= '/urls' %>">URLs</a>
     | 
    <a href="<%= '/urls/new' %>">Create New TinyUrl</a><br>
    Email: <%= user.email %>
    <form action="/logout" method="POST" class="inline">
        <button class="float-left submit-button" >Logout</button><br><br>
    </form>
<% } %>